{
  "name": "leidy-cleaner-backend",
  "version": "2.0.0",
  "description": "Backend API for Leidy Cleaner booking platform - Enhanced Edition",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "docker:build": "docker build -t leidy-cleaner-backend .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "logs": "docker-compose logs -f backend"
  },
  "keywords": [
    "cleaning",
    "booking",
    "platform",
    "api",
    "nodejs",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "Leidy Cleaner Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.1.2",
    "bcryptjs": "^2.4.3",
    "@prisma/client": "^5.7.0",
    "stripe": "^14.7.0",
    "mercadopago": "^2.2.2",
    "@sendgrid/mail": "^7.7.0",
    "twilio": "^4.9.0",
    "aws-sdk": "^2.1569.0",
    "@google/maps": "^1.3.3",
    "firebase-admin": "^12.0.0",
    "express-validator": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.12",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.4",
    "hpp": "^0.2.3",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
